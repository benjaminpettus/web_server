<h1 class="container "><%- page %></h1>
<% for(var i=0; i< data.length; i++) {%>
  <div class="col s17 m7 container">
    <div class="card horizontal">
      <div class="card-image">
        <img src="http://lorempixel.com/output/nightlife-q-c-200-200-2.jpg">
      </div>
      <div class="card-stacked">
        <div class="card-content">
          <h4><a href='/albums/<%= data[i].id %>'><%= data[i].title %></a></h3>
        </div>
        <div class="card-action">
          <div class="artist-card-attr ">
            <p>
              Artist: <a href='/artists/<%= data[i].id %>'><%= data[i].name %></a>
            </p>
            <p>
              <%= data[i].count %> Songs
            </p>
          </div>
        </div>
      </div>
    </div>
  </div>
<% } %>
