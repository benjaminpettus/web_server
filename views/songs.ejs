<!DOCTYPE html>
<html>
  <head>
    <% include ./partials/head %>
    <title></title>
  </head>
  <body>
    <% include ./partials/navbar%>

    <table class="centered highlight">
      <thead>
        <h1><%- page %></h1>
        <tr>
            <th>Track No.</th>
            <th>Song</th>
            <th>Artist</th>
            <th>Album</th>
            <th>Length</th>
        </tr>
      </thead>

      <tbody>
        <% for(var i=0; i < data.length; i++) {%>
          <tr>
            <td><%= data[i].track_no %></td>
            <td><%= data[i].title %></td>
            <td><%= data[i].album_id %></td>
            <td><%= data[i].length %></td>
          </tr>
        <% } %>
      </tbody>
    </table>

    <% include ./partials/footer %>
  </body>
</html>
